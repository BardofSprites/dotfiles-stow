defaults
auth on
protocol smtp
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        ~/.msmtp.log

# account_name
account devel-gmail
host smtp.gmail.com
port 465
tls_starttls off
eval echo from $(pass mbsync/devel-gmail-email)
eval echo user $(pass mbsync/devel-gmail-email)
passwordeval "pass mbsync/devel-gmail"

account default : devel-gmail
